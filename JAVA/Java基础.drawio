        <mxCell id="SX70cxbbN6tKfyIai92h-1" value="&lt;font style=&quot;font-size: 26px;&quot;&gt;HashMap&lt;/font&gt;" style="rounded=1;whiteSpace=wrap;html=1;" parent="1" vertex="1">
